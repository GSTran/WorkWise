{
  "manifest_version": 3,
  "name": "WorkWise",
  "version": "1.0.0",
  "description": "Productivity Extension",
  "permissions": ["tabs"],
  "icons": {
    "16": "img/cat.png",
    "48": "img/cat.png",
    "128": "img/cat.png"
  },
  "host_permissions": ["https://zenquotes.io/api/"],
  "action": {
    "default_icon": {
      "16": "img/cat.png",
      "24": "img/cat.png",
      "32": "img/cat.png"
    },
    "default_title": "WorkWise",
    "default_popup": "popup.html"
  }
}
