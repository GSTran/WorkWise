{
    "manifest_version": 3,
    "name": "WorkWise",
    "description": "WorkWise, but in react",
    "version": "0.0.1",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }